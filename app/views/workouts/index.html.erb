
<div class="container-fluid mx-0">
  <div class="container ml-3 text-white">
    <p class="display-5 mt-4 ml-3">Workouts:</p>
      <%@user.workouts.each do |workout|%>
        <ul class="list-group">
          <div class="container-fluid">
            <li class="list-group-item list-group-item-dark list-group-item-action display-6" style="width: 400px;"><p><%=link_to workout.name, "/workouts/#{workout.id}"%></p></li>
          </div>
        </ul>
      <%end%>
    <div class="btn btn-primary text-white mt-3 ml-3">
      <%=link_to "Create Workout", "/workouts/new", :class => "btn"%>
    </div>
  </div>
</div>



